{
  "manifest_version": 2,
  "name": "Stacks on Stacks",
  "version": "0.1",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "browser_action": {
    "default_title": "Stacks on Stacks",
    "default_icon": "duck.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*.stackoverflow.com/questions/*", "http://stackoverflow.com/questions/*", "https://stackoverflow.com/questions/*"],
      "js": [
        "scripts/jquery.min.js",
        "scripts/content.js",
        "scripts/custom.js",
        "scripts/stylishjs.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}
